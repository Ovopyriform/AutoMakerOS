<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet ../css/AutoMaker.css?>

<AnchorPane minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="slideOutPanel" stylesheets="@../css/JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="celtech.coreUI.controllers.ModalDialogController">
    <children>
<VBox fx:id="container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
<children>
        <VBox fx:id="defaultContent">
            <children>
                <Label fx:id="dialogTitle" alignment="CENTER" prefWidth="-1.0" styleClass="titleText" text="Title" />
                <Label fx:id="dialogMessage" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" styleClass="labelText" text="This is a very long message that shows wrapping of lots of text ha ha ha blah blah blah" textAlignment="CENTER" wrapText="true" />
            </children>
        </VBox>
        <HBox fx:id="buttonHolder" alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0">
            <padding>
                <Insets />
            </padding>
            <VBox.margin>
                <Insets top="8.0" />
            </VBox.margin>
        </HBox>
</children>
</VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</AnchorPane>
